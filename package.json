{
  "name": "frontcamp-news",
  "version": "1.0.0",
  "description": "A training project for the 2018 FrontCamp Mentoring Program",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "concurrently \"npm run start:es6\" \"npm run watch:es5\"",
    "start:prod": "cross-env NODE_ENV=production concurrently \"npm run start:es6\" \"npm run watch:es5\"",
    "start:es6": "webpack-dev-server --config webpack.config.es6.js",
    "start:es5": "webpack-dev-server --config webpack.config.es5.js",
    "build": "concurrently \"npm run build:es5\" \"npm run build:es6\"",
    "build:prod": "cross-env NODE_ENV=production concurrently \"npm run build:es5\" \"npm run build:es6\"",
    "build:es5": "webpack --config webpack.config.es5.js",
    "build:es6": "webpack --config webpack.config.es6.js",
    "watch:es6": "webpack --watch --config webpack.config.es6.js",
    "watch:es5": "webpack --watch --config webpack.config.es5.js",
    "serve": "http-server ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alaksej/frontcamp-news.git"
  },
  "author": "Aliaksei Yankou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alaksej/frontcamp-news/issues"
  },
  "homepage": "https://github.com/alaksej/frontcamp-news#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "mdn-polyfills": "^5.13.0",
    "whatwg-fetch": "^3.0.0"
  }
}
